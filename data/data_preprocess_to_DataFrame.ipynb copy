   "source": [
    "train_data_dir = 'data/chest_xray/train'\n",
    "val_data_dir = 'data/chest_xray/val'\n",
    "test_data_dir = 'data/chest_xray/test'\n",
    "\n",
    "# Get all the data in the directory data/train (5216 images), and reshape them\n",
    "train_generator = ImageDataGenerator().flow_from_directory(\n",
    "        train_data_dir, \n",
    "        target_size=(64, 64), \n",
    "        batch_size=5216\n",
    "        )\n",
    "\n",
    "# Get all the data in the directory data/val (16 images), and reshape them\n",
    "val_generator = ImageDataGenerator().flow_from_directory(\n",
    "        val_data_dir, \n",
    "        target_size=(64, 64), \n",
    "        batch_size=16\n",
    "        )\n",
    "\n",
    "# Get all the data in the directory data/test (624 images), and reshape them\n",
    "test_generator = ImageDataGenerator().flow_from_directory(\n",
    "        test_data_dir, \n",
    "        target_size=(64, 64), \n",
    "        batch_size=624\n",
    "        )\n",
    "\n",
    "# Create the datasets\n",
    "train_images, train_labels = next(train_generator)\n",
    "val_images, val_labels = next(val_generator)\n",
    "test_images, test_labels = next(test_generator)"
   ]
  },
